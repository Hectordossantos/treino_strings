#include <stdio.h>

// 2. Crie um programa que calcula o comprimento de uma string (nao use a func¸ ˜ ao strlen).

void function(char string[]);

int main(){
char str [25];
printf("digite seu nome");
scanf("%s", &str);

function(str);
return 0;
}

void function(char string[]){
int cont;
while (string[cont] != '\0')
{
    cont ++;
}
printf("numero de letras :%i",cont );

}

